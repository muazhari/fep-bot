{
  "name": "fep-bot",
  "version": "1.0.0",
  "description": "Freshmen Enrichment Program Automation Bot for Line",
  "repository": "https://github.com/muazhari/fep-bot",
  "author": "Muhammad Kharisma Azhari <kharisma.azhari02@gmail.com>",
  "license": "Modified MIT",
  "main": "dist/index.js",
  "engines": {
    "node": "~10.15.1",
    "npm": ">=6.4.0"
  },
  "scripts": {
    "prestart": "npm run -s build",
    "run": "node dist/index.js",
    "start": "bash start.sh",
    "dev": "nodemon src/index.js --exec \"node -r dotenv/config -r babel-register\"",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir -p dist && babel src -s -D -d dist",
    "test": "jest --watch",
    "lint": "esw -w src test"
  },
  "keywords": [
    "express",
    "babel",
    "boilerplate",
    "scaffold",
    "es6",
    "es2015",
    "es2016",
    "es2017",
    "jest",
    "eslint"
  ],
  "dependencies": {
    "@line/bot-sdk": "^6.8.3",
    "axios": "^0.19.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.17.0",
    "dialogflow": "^0.14.1",
    "express": "^4.17.1",
    "firebase-admin": "^8.8.0",
    "fs-extra": "^8.1.0",
    "googleapis": "^45.0.0",
    "morgan": "^1.9.1",
    "node-persist": "^3.0.5",
    "pug": "^2.0.4",
    "readline": "^1.3.0",
    "rimraf": "^3.0.0",
    "uuid": "^3.3.3",
    "youtube-dl": "^2.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-register": "^6.26.0",
    "dotenv": "^7.0.0",
    "eslint": "^5.15.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-watch": "^5.0.1",
    "jest": "^24.5.0",
    "nodemon": "^1.18.10",
    "supertest": "^4.0.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "import",
      "jest"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": true
        }
      ],
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true,
          "allowTemplateLiterals": false
        }
      ],
      "jsx-quotes": [
        "error",
        "prefer-double"
      ],
      "camelcase": "off",
      "no-use-before-define": "off",
      "semi": [
        "error",
        "never"
      ],
      "import/prefer-default-export": "off",
      "import/no-named-as-default-member": [
        0
      ],
      "jest/no-focused-tests": 2,
      "jest/no-identical-title": 2
    }
  },
  "jest": {
    "testEnvironment": "node"
  }
}
